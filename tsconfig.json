{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs", // alterado de "ESNext" para "commonjs"
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node",
      "http-cache-semantics",
      "ms"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "*.csv",
    "*.html",
    "*.css"
  ]
}